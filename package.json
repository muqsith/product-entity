{
  "name": "product-entity",
  "version": "1.0.0",
  "description": "REST/GQL API for Product entity",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "export NODE_ENV=test && npm run test-setup; jest",
    "start": "export NODE_ENV=development && npm run development-setup && npm run server-build && npm-run-all --parallel tsc-watch server-watch",
    "tsc-watch": "./node_modules/.bin/tsc -w",
    "tsc": "./node_modules/.bin/tsc",
    "server-watch": "./node_modules/.bin/nodemon dist/server.js",
    "server-build": "./node_modules/.bin/rimraf dist && npm run tsc;",
    "development-setup": "node scripts/db.js -u product_dev -p 123456 -d product_entity -c postgresql://admin:admin@localhost:5432",
    "test-setup": "node scripts/db.js -u product_test -p 123456 -d test_product_entity -c postgresql://admin:admin@localhost:5432"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "http-errors": "^2.0.0",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.5.1",
    "@types/joi": "^17.2.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "commander": "^9.3.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  }
}
